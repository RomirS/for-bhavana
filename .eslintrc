{
  "env": {
    "commonjs": true,
    "es6": false,
    "node": true
  },
  "extends": ["airbnb", "airbnb/hooks", "plugin:prettier/recommended"],
  "parserOptions": {},
  "rules": {
    "no-console": "off",
    "no-unused-vars": "warn",
    "react/no-unescaped-entities": "off",
    "no-useless-escape": "off",
    "react/prop-types": "off",
    "react/no-array-index-key": "off"
  }
}